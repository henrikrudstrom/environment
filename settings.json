  "vim.easymotionMarkerForegroundColorTwoChar": "#eeeeee",
  "vim.easymotionKeys": "tnserigkaoplhdfuc,mvjbwyx.",
  "workbench.editor.showTabs": true,
  "eslint.autoFixOnSave": true
