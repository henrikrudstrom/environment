  "peacock.surpriseMeOnStartup": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "workbench.colorTheme": "Monokai Classic",
  "vim.camelCaseMotion.enable": true,
  "editor.minimap.enabled": false,
  "todo-tree.showInExplorer": false,
  "java.dependency.showOutline": false,
  "todo-tree.hideTreeWhenEmpty": false,
  "metaGo.bookmark.saveBookmarksInProject": true,
  "metaGo.decoration.fontFamily": "Fira Code",
  "metaGo.decoration.width": 10,
  "metaGo.decoration.y": 13,
  "peacock.affectActivityBar": false,
  "peacock.affectTitleBar": false,
  "git-graph.showStatusBarItem": false,
  "gitlens.mode.statusBar.alignment": "left",
  "peacock.showColorInStatusBar": false,
  "eslint.alwaysShowStatus": true,
  "workbench.activityBar.visible": false,
  "editor.formatOnPaste": true,
  "prettier.requireConfig": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerFontFamily": "Fira Code",
  "vim.easymotionMarkerYOffset": -7,
  "vim.easymotionMarkerHeight": 17,
  "vim.easymotionMarkerWidthPerChar": 9,
  "vim.easymotionMarkerBackgroundColor": "#551e1e88",
